<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>
	<action-mappings>
		<action path="/autentica" type="br.com.edutex.form.Login"  scope="request" >
			<forward name="success" path="/WEB-INF/autenticacao/ok.jsp" />
		</action>
		
		<action path="/login" type="br.com.edutex.form.Login"  scope="request" >
			<forward name="success" path="/WEB-INF/login.jsp" />
		</action>
	</action-mappings>
</struts-config>

